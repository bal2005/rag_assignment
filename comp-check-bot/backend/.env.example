# Backend environment variables
# Copy to comp-check-bot/backend/.env and fill in your values
# Do NOT commit the .env file to source control

# ── Neon / Postgres ─────────────────────────────────────────
DB_USER_NEON=
DB_PW_NEON=
DB_NEON_HOST=
DB_NEON_NAME=

# ── Milvus / Zilliz Cloud ────────────────────────────────────
MILVUS_URI=
MILVUS_API_KEY=
MILVUS_COLLECTION=legal_policy_vectors

# ── HuggingFace Embeddings ───────────────────────────────────
HF_TOKEN=

# ── Groq LLM ────────────────────────────────────────────────
GROQ_API_KEY=
GROQ_MODEL=openai/gpt-oss-20b

# ── Application ──────────────────────────────────────────────
PORT=8000
ALLOWED_ORIGINS=*
TOP_K=5
